{
  "compilerOptions": {
    "target": "es2022",
    "module": "node16",
    "lib": ["es2023"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "outDir": "./dist",
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts"],
}